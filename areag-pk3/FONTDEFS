    z TABLLOWZ
    0 TABLNUM0
    1 TABLNUM1
    2 TABLNUM2
    3 TABLNUM3
    4 TABLNUM4
    5 TABLNUM5
    6 TABLNUM6
    7 TABLNUM7
    8 TABLNUM8
    9 TABLNUM9
    & TABLAMPE
    * TABLASTR
    | TABLBAR
    \ TABLBSLA
    / TABLFSLA
    ^ TABLCART
    , TABLCOMA
    $ TABLDOLL
    "\"" TABLDQUO
    ' TABLSQUO
    = TABLEQUA
    ! TABLEXCL
    ` TABLGRAV
    ~ TABLTILD
    //# TABLHASH
    [ TABLLBRA
