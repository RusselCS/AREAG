    < TABLLEST
    > TABLGRET
    ( TABLLPAR
    ) TABLLPAR
    - TABLMINU
    % TABLPERC
    + TABLPLUS
    ? TABLQUES
    ; TABLSEMI
    @ TABLSYAT
    _ TABLUNDR
	. TABLPERI
}

